          </ul>
        </div>

        <!-- Tab End -->
      </div>
      <br>
      <div class="row">
        <div class="col">
          <div class="tab-content">
            <!-- 1st tab start -->
            <div class="tab-pane fade show active" id="tab-product-new-arrivals">
              <%include ./product-list-bed %>
            </div>
            <!-- 1st tab end -->
            <!-- 2nd tab start -->
            <div class="tab-pane fade" id="tab-product-best-sellers">
              <%include ./product-list-living %>
            </div>
            <!-- 2nd tab end -->
            <!-- 3rd tab start -->
            <div class="tab-pane fade" id="tab-product-sale-item">
              <%include ./product-list-storage %>
            </div>
            <!-- 3rd tab end -->
          </div>
        </div>
      </div>
    </div>
  </div><br>
